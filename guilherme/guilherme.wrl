 #	______________________________ Definição das bocas do Fogão

Transform
{
	translation -1.550 -0.45 2.125
	#rotation 0 0 1 3.14
	children
	[

		
		DEF BocaFogao Group	{children [

			Transform {translation 0 0.001 0.0875
			children  [
			Shape {
					appearance Appearance {
						material Material {
							diffuseColor 0.8 0.8 0.8
                				}
					}
					geometry Cylinder {
						radius	0.05
						height	0.015
						side	TRUE
						top 	TRUE
						bottom	TRUE
						}
					}
				]}
		]}


#Objeto_______________  Base do FOGÃO
		Shape
		{
			appearance Appearance {	
						material Material {
							diffuseColor 0 0 0
                					}
			}
			geometry Box { size 0.55 0.004 0.355 }
		}
		
		
		Transform {translation 0 0 -0.175
						children  USE BocaFogao}
			

		Transform {translation 0.175 0 -0.175
						children  USE BocaFogao}
			
		Transform {translation -0.175 0 -0.175
						children  USE BocaFogao}


		Transform {translation -0.175 0 0
						children  USE BocaFogao}
		 		 
		Transform {translation 0.175 0 0
						children  USE BocaFogao}

	]
}

