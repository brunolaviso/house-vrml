	translation	1.33 -0.32 5.25 
	children [

	Shape 
	{  
		appearance Appearance 
		{  	
			material Material{ diffuseColor 0.7 0.5 0.3 }
			texture	ImageTexture { url "textura.jpg" }
		}
		geometry Box { size 0.8 2.10 0.10 }
	}
	]
}

